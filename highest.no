//To create a program to input 2 numbers and find the highest
#include<stdio.h>
int main(){
   int x,y;
   printf("Enter 2 numbers");
   scanf("%d%d",&x,&y);
   if(x>y)
   {
       printf("%d is greater",x);
   }
   else
   {
       printf("%d is greater",y);
   }
   return 0;
   }
   
